{
  "name": "shtcut-ui",
  "version": "1.0.0",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*",
      "toolings/*",
      "docs"
    ]
  },
  "description": "",
  "author": "Funmi Ayinde",
  "license": "MIT",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "prepare": "husky install",
    "clean": "learn clean --yes && rm -rf node_modules",
    "clean-dist": "yarn lerna exec -- rimraf dist",
    "prestart": "yarn && yarn bootstrap",
    "boot": "yarn prestart && yarn storybook",
    "start": "yarn build && yarn storybook",
    "start:all": "yarn build && concurrently \"yarn docs dev\" \"yarn storybook\" ",
    "build": "lerna run build --no-private --stream",
    "bootstrap": "yarn install && learn bootstrap --use-workspaces",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "release": "changeset publish",
    "docs": "yarn workspace @shtcut-ui/docs",
    "docs:dev": "yarn docs dev",
    "docs:start": "yarn docs start"
  },
  "resolutions": {
    "**/react": "18.2.0",
    "**/react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/runtime": "^7.23.7",
    "@changesets/cli": "^2.27.1",
    "@storybook/addon-essentials": "^7.6.7",
    "@storybook/addon-interactions": "^7.6.7",
    "@storybook/addon-links": "^7.6.7",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/blocks": "^7.6.7",
    "@storybook/cli": "^7.6.7",
    "@storybook/nextjs": "^7.6.7",
    "@storybook/react": "^7.6.7",
    "@storybook/test": "^7.6.7",
    "@tailwindcss/forms": "^0.5.7",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "lerna": "^8.0.1",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "storybook": "^7.6.7",
    "storybook-addon-performance": "^0.17.1",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "tsup": "^8.0.1"
  }
}
